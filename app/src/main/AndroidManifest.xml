<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nextcode">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_index_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_index_foreground"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar"
        android:usesCleartextTraffic="true">
        <activity android:name=".views.FacturaGeneradaActivity"></activity>
        <activity android:name=".views.FacturasActivity" />
        <activity android:name=".views.PlanesDeUsuariosActivity" />
        <activity android:name=".views.PlanesActivity" />
        <activity android:name=".views.IngresoMenuActivity" />
        <activity android:name=".views.RegistroActivity" />
        <activity android:name=".views.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>